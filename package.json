{
  "name": "googlebookssearch",
  "version": "1.0.0",
  "description": "React-based Google Books Search app",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/germangib/GoogleBooksSearch.git"
  },
  "author": "German Garcia",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.0"
  },
  "bugs": {
    "url": "https://github.com/germangib/GoogleBooksSearch/issues"
  },
  "homepage": "https://github.com/germangib/GoogleBooksSearch#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.4",
    "if-env": "^1.0.4",
    "logger": "0.0.1",
    "mongoose": "^5.5.7",
    "morgan": "^1.9.1",
    "path": "^0.12.7",
    "react": "^16.8.6"
  }
}
